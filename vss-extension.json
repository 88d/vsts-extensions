{
  "manifestVersion": 1,
  "id": "createdocumentation-extension",
  "version": "0.2.5",
  "name": "Create Documentation Extension",
  "scopes": [ "vso.build" ],
  "description": "This extension adds a create documentation task.",
  "baseUri": "https://localhost",
  "publisher": "black-banana",
  "public": false,
  "icons": {
    "default": "img/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "Documentation"
  ],
  "links": {
    "home": {
      "uri": "https://github.com/black-banana/vsts-extensions"
    },
    "getstarted": {
      "uri": "https://github.com/black-banana/vsts-extensions"
    },
    "repository": {
      "uri": "https://github.com/black-banana/vsts-extensions"
    },
    "issues": {
      "uri": "https://github.com/black-banana/vsts-extensions/issues"
    }
  },
  "branding": {
    "color": "rgb(255, 102, 0)",
    "theme": "light"
  },
  "files": [
    {
      "path": "CreateDocumentationTask"
    }
  ],
  "categories": [
    "Build and release"
  ],
  "contributions": [
    {
      "id": "createdocumentation-task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "CreateDocumentationTask"
      }
    }
  ]
}
