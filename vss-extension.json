{
  "manifestVersion": 1,
  "id": "createdocumentation-extension",
  "version": "0.2.4",
  "name": "Create Documentation Extension",
  "scopes": [ "vso.build" ],
  "description": "This extension adds the createdocumentation Task.",
  "baseUri": "https://localhost",
  "publisher": "pmone",
  "public": false,
  "icons": {
    "default": "img/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "Documentation"
  ],
  "links": {
    "home": {
      "uri": "https://bit.ly"
    },
    "getstarted": {
      "uri": "https://bit.ly"
    },
    "learn": {
      "uri": "https://bit.ly"
    },
    "support": {
      "uri": "https://bit.ly"
    },
    "repository": {
      "uri": "https://bit.ly"
    },
    "issues": {
      "uri": "https://bit.ly"
    }
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "files": [
    {
      "path": "CreateDocumentationTask"
    }
  ],
  "categories": [
    "Build and release"
  ],
  "contributions": [
    {
      "id": "createdocumentation-task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "CreateDocumentationTask"
      }
    }
  ]
}
