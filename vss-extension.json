{
    "manifestVersion": 1,
    "id": "blackbanana-build-extensions",
    "version": "0.7.0",
    "name": "Blackbanana Build Extensions",
    "scopes": ["vso.build"],
    "description": "This extension adds multiple tasks to help with vsts.",
    "baseUri": "https://localhost",
    "publisher": "black-banana",
    "public": false,
    "icons": {
        "default": "img/logo.png"
    },
    "targets": [{
        "id": "Microsoft.VisualStudio.Services"
    }],
    "tags": [
        "Documentation",
        "BuildArtifacts"
    ],
    "links": {
        "home": {
            "uri": "https://github.com/88d/vsts-extensions"
        },
        "getstarted": {
            "uri": "https://github.com/88d/vsts-extensions"
        },
        "repository": {
            "uri": "https://github.com/88d/vsts-extensions"
        },
        "issues": {
            "uri": "https://github.com/88d/vsts-extensions/issues"
        }
    },
    "branding": {
        "color": "rgb(255, 102, 0)",
        "theme": "light"
    },
    "files": [{
        "path": "CreateDocumentationTask"
    }, {
        "path": "DownloadBuildArtifactsTask"
    }, {
        "path": "SharePointUploadTask"
    }, {
        "path": "SignSetupTask"
    }],
    "categories": [
        "Build and release"
    ],
    "contributions": [{
            "id": "createdocumentation-task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "type": "ms.vss-distributed-task.task",
            "properties": {
                "name": "CreateDocumentationTask"
            }
        },
        {
            "id": "downloadbuildartifacts-task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "type": "ms.vss-distributed-task.task",
            "properties": {
                "name": "DownloadBuildArtifactsTask"
            }
        },
        {
            "id": "sharepointupload-task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "type": "ms.vss-distributed-task.task",
            "properties": {
                "name": "SharePointUploadTask"
            }
        }, {
            "id": "signsetup-task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "type": "ms.vss-distributed-task.task",
            "properties": {
                "name": "SignSetupTask"
            }
        }
    ]
}